# Mission Control API — environment variables
# Copy to .env and fill in values

# Database (required) — falls back to MC_DB_PATH from shared env
MC_API_DB_PATH=

# Supervisor System path — falls back to SUPERVISOR_SYSTEM_PATH from shared env
MC_API_SUPERVISOR_SYSTEM_PATH=

# Langfuse credentials — falls back to LANGFUSE_* from shared env
MC_API_LANGFUSE_HOST=
MC_API_LANGFUSE_PUBLIC_KEY=
MC_API_LANGFUSE_SECRET_KEY=

# CORS origins (comma-separated)
MC_API_CORS_ORIGINS=["http://localhost:3000","http://localhost:3100"]
